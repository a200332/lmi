                             |"B=+50%"
                             |"C=+75%"
                             |"D=+100%"
                             |"E=+125%"
                             |"F=+150%"
                             |"H=+200%"
                             |"J=+250%"
                             |"L=+300%"
                             |"P=+400%"},
